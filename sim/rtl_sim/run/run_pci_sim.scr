ncvlog -file ../bin/ncvlog_rtl.args
ncvlog -file ../bin/ncvlog_sim.args

if [[ $1 == 'xilinx' ]]; then 
    export XILINX=../../../../lib/xilinx
    source ../bin/ncvlog_xilinx.scr; 
    ncelab -file ../bin/ncelab_xilinx.args;
elif [[ $1 == 'artisan' ]]; then
    export ARTISAN=../../../../lib/artisan/art_hsdp_40x256
    source ../bin/ncvlog_artisan.scr;
fi

